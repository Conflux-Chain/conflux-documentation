{
  "name": "conflux-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "front-matter": "node scripts/add-front-matter.js",
    "start": "yarn front-matter && docusaurus start",
    "build": "yarn front-matter && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "yarn front-matter && docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "yarn front-matter && docusaurus serve",
    "zh": "yarn start --locale zh-CN",
    "es": "yarn start --locale es",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@cookbookdev/docsbot": "^4.5.0",
    "@docusaurus/core": "^3.1.0",
    "@docusaurus/plugin-ideal-image": "^3.1.0",
    "@docusaurus/preset-classic": "^3.1.0",
    "@mdx-js/react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "clsx": "^1.2.1",
    "postcss": "^8.4.18",
    "prism-react-renderer": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "tailwindcss": "^3.2.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.0.0",
    "@docusaurus/types": "3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
